{
  "/search/code/:language/:term": {
    "GET": {
      "route": "moduleController:getSearchCode",
      "policy": [
        "firewall:checkSearchParams"
      ],
      "cors": true
    }
  },
  "/syntax/check": {
    "POST": {
      "route": "moduleController:postCheckSyntax",
      "cors": true
    }
  }
}